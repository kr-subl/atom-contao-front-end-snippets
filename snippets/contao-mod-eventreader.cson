  'Contao Mod Eventreader':
    'prefix': 'mod_eventreader'
    'body': """
      .mod_eventreader {
        ${1}
        .event {
          ${2}
        }

        .layout_full {
          ${3}
        }

        .layout_list {
          ${4}
        }

        .layout_teaser {
          ${5}
        }

        .layout_upcoming {
          ${6}
        }

        .header {
          ${7}
        }

        .time {
          ${8}
        }

        .day {
          ${9}
        }

        .location {
          ${10}
        }

      }
    """

'.source.sass, .source.styl':
  'Contao Mod Eventreader':
    'prefix': 'mod_eventreader'
    'body': """
      .mod_eventreader
        ${1}
        .event
          ${2}
        .layout_full
          ${3}
        .layout_list
          ${4}
        .layout_teaser
          ${5}
        .layout_upcoming
          ${6}
