'.source.css':
  'Contao Mod Quicklink':
    'prefix': 'mod_quicklink'
    'body': """
      .mod_quicklink {
        ${1}
      }

      .mod_quicklink .formbody {
        ${2}
      }

      .mod_quicklink .select {
        ${3}
      }

      .mod_quicklink .submit {
        ${4}
      }
    """

'.source.less, .source.scss':
  'Contao Mod Quicklink':
    'prefix': 'mod_quicklink'
    'body': """
      .mod_quicklink {
        ${1}
        .formbody {
          ${2}
        }

        .select {
          ${3}
        }

        .submit {
          ${4}
        }

      }
    """

'.source.sass, .source.styl':
  'Contao Mod Quicklink':
    'prefix': 'mod_quicklink'
    'body': """
      .mod_quicklink
        ${1}
        .formbody
          ${2}
        .select
          ${3}
        .submit
          ${4}
    """
