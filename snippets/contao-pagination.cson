  'Contao Pagination':
    'prefix': 'pagination'
    'body': """
      .pagination {
        ${1}
        p {
          ${2}
        }

        ul {
          ${3}
        }

        li {
          ${4}
        }

        a {
          ${5}
        }

        span {
          ${6}
        }

        .link {
          ${7}
        }

        .previous {
          ${8}
        }

        .next {
          ${9}
        }

        .active,
        .current {
          ${10}
        }

      }
    """

'.source.sass, .source.styl':
  'Contao Pagination':
    'prefix': 'pagination'
    'body': """
      .pagination
        ${1}
        p
          ${2}
        ul
          ${3}
        li
          ${4}
        a
          ${5}
        span
          ${6}
        .link
          ${7}
        .previous
          ${8}
        .next
          ${9}
        .active,
        .current
          ${10}
    """
